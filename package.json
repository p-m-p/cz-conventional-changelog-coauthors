{
  "name": "cz-conventional-changelog-coauthors",
  "version": "0.0.0-development",
  "description": "cz-conventional-changelog with co-authors prompt",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "prettier": "prettier --write .",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "git",
    "github",
    "commit",
    "commitizen",
    "cz",
    "cz-cli",
    "co-authored-by"
  ],
  "author": "Phil Parsons",
  "license": "MIT",
  "engines": {
    "node": ">= 16"
  },
  "dependencies": {
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0"
  },
  "devDependencies": {
    "eslint": "^8.43.0",
    "prettier": "^2.8.8",
    "semantic-release": "^21.0.5"
  },
  "config": {
    "commitizen": {
      "path": "./index.js"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/p-m-p/cz-conventional-changelog-coauthors.git"
  }
}
